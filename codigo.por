programa {
  funcao inicio() {
    
    inteiro n1, n2, soma, subtracao, multiplicacao
    escreva("Digite um numero: ")
    leia(n1)
    escreva("Digite um numero: ")
    leia(n2)

    soma = n1 + n2
    subtracao = n1 - n2
    multiplicacao = n1 * n2

    escreva("A soma é ",soma ,"\n")
    escreva("A subtração é: ", subtracao)
    escreva("\nA multiplicação é: ", multiplicacao)
  }
}
